{
  "name": "work_refactor_aws",
  "version": "1.0.0",
  "description": "Projeto de refatoração API para AWS",
  "main": "index.js",
  "author": "Ivan Barros",
  "license": "MIT",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:open:staging": "cross-env CYPRESS_BASE_URL=https://stg-amei.amorsaude.tech cypress open",
    "cypress:open:homologacao": "cross-env CYPRESS_BASE_URL=https://amei-homolog.amorsaude.com.br cypress open",
    "cypress:run:homologacao": "cross-env CYPRESS_BASE_URL=https://amei-homolog.amorsaude.com.br cypress run",
    "cypress:open:producao": "cross-env CYPRESS_BASE_URL=https://amei.amorsaude.com.br cypress open",
    "test:perfil-acesso": "cypress run --spec 'cypress/e2e/AWS/perfil_acesso.cy.js'",
    "test:origens": "cypress run --spec 'cypress/e2e/AWS/origens.cy.js'",
    "test:cartao-todos": "cypress run --spec 'cypress/e2e/AWS/cartao_de_todos.cy.js'",
    "test:cartao-todos-integration": "cypress run --spec 'cypress/e2e/AWS/cartao_de_todos_integration.cy.js'",
    "test:recursos": "cypress run --spec 'cypress/e2e/AWS/recursos.cy.js'",
    "test:plano-contas": "cypress run --spec 'cypress/e2e/AWS/plano_de_contas.cy.js'",
    "test:grupo-regras": "cypress run --spec 'cypress/e2e/AWS/grupo_de_regras_acesso.cy.js'",
    "test:fuso-horario": "cypress run --spec 'cypress/e2e/AWS/fuso_horario.cy.js'",
    "test:tipo-documentos": "cypress run --spec 'cypress/e2e/AWS/tipo_documentos.cy.js'",
    "test:portes-tabelas": "cypress run --spec 'cypress/e2e/AWS/portes_tabelas.cy.js'",
    "test:agendar": "cypress run --spec \"cypress/e2e/AWS/modulos/AGENDAR/{cartao_de_todos_integration,cartao_de_todos,restricoes,status_agendamento}.cy.js\"",
    "test:atender": "cypress run --spec \"cypress/e2e/AWS/modulos/ATENDER/classificacao_medica.cy.js\"",
    "test:cadastrar": "cypress run --spec \"cypress/e2e/AWS/modulos/CADASTRAR/{canais,conselho_profissional,convenios,especialidades,fornecedor_integracao,funcionarios_funcoes,funcoes,fuso_horario,grupo_de_regras_acesso,login,municipio_estado,origens,parceiro_institucional,parcerias,perfil_acesso,perfis,portes_tabelas,profissoes,recursos,refresh_token,regimes_tributario,regioes,setores,tipo_documentos,tipo_segmento,tipo_unidade,venda_campanha}.cy.js\"",
    "test:coletar": "cypress run --spec \"cypress/e2e/AWS/modulos/coletar/{dashboard_profissionais,vinculo_unidade_laboratorio}.cy.js\"",
    "test:faturar": "cypress run --spec \"cypress/e2e/AWS/modulos/faturar/{cadeado,fechar_cadeado,pagamento_faturado,painel_de_controle,parametros,plano_de_contas,royalties}.cy.js\"",
    "test:produzir": "cypress run --spec \"cypress/e2e/AWS/modulos/produzir/report.cy.js\""
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "cypress": "^12.17.4",
    "cypress-plugin-api": "^2.11.2",
    "rimraf": "^5.0.10"
  },
  "dependencies": {
    "cypress-plugin-api": "^2.11.2"
  }
}